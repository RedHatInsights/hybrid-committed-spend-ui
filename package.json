{
  "name": "hybrid-committed-spend",
  "version": "1.1.0",
  "private": false,
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "scripts": {
    "build": "npm run build:prod",
    "build:prod": "NODE_ENV=production fec build --webpack-config config/dev.webpack.config.js",
    "check:updates": "npx npm-check-updates",
    "check:milestone": "npx npm-check-updates -t newest -f '/^(@patternfly|@redhat-cloud-services)/'",
    "check:milestone:upgrade": "npx npm-check-updates -t newest -f '/^(@patternfly|@redhat-cloud-services)/' -u",
    "clean": "rimraf dist/",
    "deploy": "npm-run-all build lint test",
    "lint": "npm-run-all lint:*",
    "lint:ts": "eslint config src",
    "lint:ts:fix": "eslint config src --fix",
    "lint:sass": "stylelint 'src/**/*.scss' --config .stylelintrc.json",
    "patch:hosts": "fec patch-etc-hosts",
    "start": "fec dev --webpack-config config/dev.webpack.config.js",
    "start:csc": "CONFIG_PORT=8889 npm start",
    "test": "jest --no-cache",
    "test:clean": "jest --clearCache",
    "test:update": "yarn test:clean && jest --updateSnapshot",
    "translations": "npm-run-all translations:extract translations:compile translations:compile:ast translations:datafile",
    "translations:test": "npm-run-all translations:extract translations:compile translations:compile:ast 'translations:syncTranslations --locale fr' 'translations:syncTranslations --locale de' translations:datafile",
    "translations:clean": "rm -f `/bin/ls -1 locales/*.json | /usr/bin/egrep -v 'data|translations'.json`",
    "translations:extract": "npx formatjs extract src/locales/*.ts --out-file build/messages/src/Messages.json",
    "translations:compile": "npx formatjs compile build/messages/src/Messages.json --out-file locales/translations.json",
    "translations:compile:ast": "npx formatjs compile build/messages/src/Messages.json --out-file locales/en.json --ast",
    "translations:datafile": "node scripts/createDataJson.js",
    "translations:syncTranslations": "node scripts/syncTranslations.js",
    "verify": "npm-run-all build lint test"
  },
  "dependencies": {
    "@patternfly/patternfly": "4.215.1",
    "@patternfly/react-charts": "6.94.4",
    "@patternfly/react-core": "4.239.0",
    "@patternfly/react-icons": "4.90.0",
    "@patternfly/react-tokens": "4.91.0",
    "@redhat-cloud-services/frontend-components": "^3.9.11",
    "@redhat-cloud-services/frontend-components-notifications": "^3.2.10",
    "@redhat-cloud-services/frontend-components-translations": "^3.2.4",
    "@redhat-cloud-services/frontend-components-utilities": "^3.2.25",
    "@unleash/proxy-client-react": "^3.3.0",
    "axios": "^0.27.2",
    "classnames": "^2.3.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intl": "^6.1.2",
    "react-redux": "^7.2.9",
    "react-router-dom": "5.3.3",
    "redux": "^4.2.0",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^6.1.3",
    "redux-thunk": "^2.4.1",
    "typesafe-actions": "^5.1.0",
    "victory-core": "36.6.7"
  },
  "devDependencies": {
    "@formatjs/cli": "^5.1.1",
    "@redhat-cloud-services/eslint-config-redhat-cloud-services": "^1.2.3",
    "@redhat-cloud-services/frontend-components-config": "^4.6.18",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@types/react": "17.0.48",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/redux-logger": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "aphrodite": "^2.4.0",
    "eslint": "8.22.0",
    "eslint-plugin-formatjs": "^4.3.2",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-patternfly-react": "^4.80.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-testing-library": "^5.7.2",
    "git-revision-webpack-plugin": "^5.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "npm-check-updates": "^16.3.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "stylelint": "^14.13.0",
    "stylelint-config-recommended-scss": "^7.0.0",
    "stylelint-scss": "^4.3.0",
    "ts-loader": "^9.4.1",
    "typescript": "^4.8.4",
    "webpack-bundle-analyzer": "^4.6.1"
  },
  "overrides": {
    "eslint": "8.22.0"
  },
  "insights": {
    "appname": "hybrid-committed-spend"
  }
}
